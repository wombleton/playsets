ol.table
  - for (var i = 0; i < 6; i++)
    li.category
      .heading
        span.label
          =(i+1)
          |  
          -if (table[i].header)
            =table[i].header
          - else
            span.empty Empty
        |  
        a.edit(href=playset.url + '/' + property + '/' + i) Edit
        
      ol.entries
        - for (var j = 0; j < 6; j++)
          li(class='entry ' + (j + 1))
            =(j+1)
            |  
            - if (table[i][j])
              =table[i][j]
            - else
              span.empty Empty